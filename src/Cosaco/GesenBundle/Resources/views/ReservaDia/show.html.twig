{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ReservaDia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Actividadreserva</th>
                <td>{{ entity.actividadReserva }}</td>
            </tr>
            <tr>
                <th>Observacionreserva</th>
                <td>{{ entity.observacionReserva }}</td>
            </tr>
            <tr>
                <th>Chequeoreserva</th>
                <td>{{ entity.chequeoReserva }}</td>
            </tr>
            <tr>
                <th>Fechaactualizacion</th>
                <td>{{ entity.fechaActualizacion|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reservas_dia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reservas_dia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
