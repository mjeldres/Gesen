                strToHtml+="</tbody>\n</table>\n</div>\n</td>\n";
                strToHtml+="<td><div id=\"gsn-calendario-contenido\">\n"+strGridReserva+"\n</div></td>\n";
                strToHtml+="</tr>\n</tbody>\n</table>\n</div>\n";
                
                
