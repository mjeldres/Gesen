                var txtInf=$('#gsn-calendario-horas>table>tbody>tr:eq('+(pInf-1)+')>td:nth-child(1)').data('htermino');
                var txtHeader=txtSup+ "-\n" + txtInf;
                
                $(this).find(".panel-title").text(txtHeader);
                //console.log(txtHeader);
            }
            
